{
  "name": "my-gridsome-site",
  "scripts": {
    "test": "jest",
    "build": "gridsome build",
    "develop": "gridsome develop",
    "explore": "gridsome explore",
    "commit": "git-cz",
    "precommit": "lint-staged",
    "format": "fixpack",
    "gen:vue": "hygen generate vue",
    "gen:store": "hygen generate store",
    "gen:util": "hygen generate util"
  },
  "config": {
    "commitizen": {
      "path": "./cz-custom"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.22",
    "@fortawesome/free-brands-svg-icons": "^5.10.2",
    "@fortawesome/free-solid-svg-icons": "^5.10.2",
    "@fortawesome/vue-fontawesome": "^0.1.7",
    "@gridsome/plugin-critical": "^0.1.3",
    "@gridsome/plugin-sitemap": "^0.2.1",
    "@gridsome/source-contentful": "^0.4.0",
    "gridsome": "0.6.9",
    "gsap": "^2.1.3",
    "prismjs": "^1.17.1",
    "vue-markdown": "^2.2.4",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "autoprefixer": "^9.6.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.9.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-vue": "^2.0.2",
    "commitizen": "^4.0.3",
    "eslint": "^6.3.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.2.0",
    "eslint-plugin-gridsome": "^1.2.8",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-vue": "^5.2.3",
    "fixpack": "^2.3.1",
    "hygen": "^4.0.9",
    "jest": "^24.9.0",
    "jest-serializer-vue": "^2.0.2",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^9.2.5",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.3",
    "postcss-loader": "^3.0.0",
    "prettier": "1.14.3",
    "sass-loader": "^8.0.0",
    "sass-resources-loader": "^2.0.1",
    "stylelint": "10.1.0",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-config-recess-order": "^2.0.3",
    "stylelint-config-sass-guidelines": "^6.0.0",
    "vue-eslint-parser": "^6.0.4",
    "vue-jest": "^3.0.4"
  },
  "private": true,
  "lint-staged": {
    "*.scss": [
      "prettier --write",
      "stylelint --fix --cache --cache-location '.stylelintcache'",
      "git add"
    ],
    "*.js": [
      "eslint --fix --cache --cache-location '.eslintcache'",
      "git add"
    ],
    "*.vue": [
      "stylelint --fix --cache --cache-location '.stylelintcache'",
      "eslint --fix --cache --cache-location '.eslintcache'",
      "git add"
    ],
    "*.md": [
      "prettier --write",
      "git add"
    ]
  }
}
