{
  "name": "my-gridsome-site",
  "scripts": {
    "test": "jest",
    "build": "gridsome build",
    "develop": "gridsome develop",
    "explore": "gridsome explore",
    "commit": "git-cz",
    "precommit": "lint-staged",
    "format": "fixpack",
    "make:vue": "hygen generate vue",
    "make:store": "hygen generate store",
    "make:util": "hygen generate util"
  },
  "config": {
    "commitizen": {
      "path": "./cz-custom"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.15",
    "@fortawesome/free-brands-svg-icons": "^5.7.2",
    "@fortawesome/free-solid-svg-icons": "^5.7.2",
    "@fortawesome/vue-fontawesome": "^0.1.5",
    "@gridsome/plugin-critical": "^0.1.0",
    "@gridsome/plugin-sitemap": "^0.1.0",
    "@gridsome/source-contentful": "^0.2.1",
    "gridsome": "0.5.0",
    "gsap": "^2.1.1",
    "prismjs": "^1.15.0",
    "vue-markdown": "^2.2.4",
    "vuex": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/preset-env": "^7.3.1",
    "@vue/test-utils": "^1.0.0-beta.29",
    "autoprefixer": "^9.4.8",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.1.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-vue": "^2.0.2",
    "commitizen": "^3.0.7",
    "eslint": "^5.14.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-gridsome": "^1.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-vue": "^5.2.2",
    "fixpack": "^2.3.1",
    "hygen": "^2.1.2",
    "jest": "^24.1.0",
    "jest-serializer-vue": "^2.0.2",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^8.1.4",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.3",
    "postcss-loader": "^3.0.0",
    "prettier": "1.14.3",
    "sass-loader": "^7.1.0",
    "sass-resources-loader": "^2.0.0",
    "stylelint": "9.10.1",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-recess-order": "^2.0.1",
    "stylelint-config-sass-guidelines": "^5.2.0",
    "vue-eslint-parser": "^6.0.2",
    "vue-jest": "^3.0.3"
  },
  "private": true,
  "lint-staged": {
    "linters": {
      "*.scss": [
        "prettier --write",
        "stylelint --fix --cache --cache-location '.stylelintcache'",
        "git add"
      ],
      "*.js": [
        "eslint --fix --cache --cache-location '.eslintcache'",
        "git add"
      ],
      "*.vue": [
        "stylelint --fix --cache --cache-location '.stylelintcache'",
        "eslint --fix --cache --cache-location '.eslintcache'",
        "git add"
      ],
      "*.md": [
        "prettier --write",
        "git add"
      ]
    },
    "ignore": [
      "node_modules/**/*",
      "cz-custom/**/*",
      "src/.temp/**/*"
    ]
  }
}